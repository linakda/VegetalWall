@page
@model StaticscreenModel
@{
  
    Layout = null;
  
}
<!DOCTYPE html>
<html>
  <head>
      <meta charset="utf-8" />
      <meta name="viewport" content="width=device-width, initial-scale=1.0" />
      <title>StaticScreen</title>
      <link href="@Url.Content("~/css/staticscreen/countdownframe.css")" rel="stylesheet" type="text/css" />
      <link href="@Url.Content("~/css/staticscreen/mediasframe.css")" rel="stylesheet" type="text/css" />
      <link href="@Url.Content("~/css/staticscreen/newsframe.css")" rel="stylesheet" type="text/css" />
      <link href="@Url.Content("~/css/staticscreen/newsframe.css")" rel="stylesheet" type="text/css" />
      <link href="@Url.Content("~/css/staticscreen/socialnetworksframe.css")" rel="stylesheet" type="text/css" />
      <link href="@Url.Content("~/css/staticscreen/staticscreen.css")" rel="stylesheet" type="text/css" />
      <link href="@Url.Content("~/css/staticscreen/wallframe.css")" rel="stylesheet" type="text/css" />
      <script type="text/javascript" src="@Url.Content("~/js/countdown.js")"></script>
      <script type="text/javascript" src="@Url.Content("~/lib/jquery/dist/jquery.js")"></script>
      <script type="text/javascript" src="@Url.Content("~/lib/transition.min.js")"></script>
  </head>
  <body>
      <div class="staticscreen-body" id="aa" >

  <div class="wallpage-frame" id="a">
    <div class="wall-block">
    <div class="wall-info">
      <div class="wall-icon"><img class="i1" src="~/images/icones/lumiere.png"/></div>
      <div class="wall-text"><p style ="color:black">Luminosité</p></div>
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>
    </div>

    <div class="wall-info">
      <div class="wall-icon"><img class="i2" src="~/images/icones/air.png"/></div>
      <div class="wall-text"><p style ="color:black">Qualité d'air</p></div>
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>
    </div>

    <div class="wall-info">
      <div class="wall-icon"><img class="i3" src="~/images/icones/pression.png"/></div>
      <div class="wall-text"><p style ="color:black">Commande Pompe</p></div> 
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>
    </div>

    <div class="wall-info">
      <div class="wall-icon"><img class="i4" src="~/images/icones/hydro.png"/></div>
      <div class="wall-text"><p style ="color:black">Humidité</p></div>
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>
    </div>

    <div class="wall-info">
      <div class="wall-icon"><img class="i5" src="~/images/icones/mouvement.png"/></div>
      <div class="wall-text"><p style ="color:black">Flux entrant/sortant</p></div>
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>
    </div>

    <div class="wall-info">
      <div class="wall-icon"><img class="i6" src="~/images/icones/pression_ruche.png"/></div>
      <div class="wall-text"><p style ="color:black">Pression ruche</p></div>
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>

    </div>

    <div class="wall-info">
      <div class="wall-icon"><img class="i7" src="~/images/icones/thermo_ruche.png"/></div>
      <div class="wall-text"><p style ="color:black">Température ruche</p></div>
      <div class="wall-graph">
        <img src="~/images/graph1.jpg"/>
      </div>
    </div>
  </div>


 
  
  </div>


<div class="newspage-frame" id="b">
    @{ Html.RenderPartial("_News"); }
</div>


<div class="countdown-frame" id = "c">
    @{ Html.RenderPartial("_Countdown"); }
</div>

<div class="mediaspage-frame" id = "d">
    @{ Html.RenderPartial("_Medias"); }
</div>


<div class="socialnetworks-frame" id ="e">
    <!-- SnapWidget -->
    <script src="https://snapwidget.com/js/snapwidget.js"></script>
    <iframe src="https://snapwidget.com/embed/698344" class="snapwidget-widget" allowtransparency="true" frameborder="0" scrolling="no" style="border:none; overflow:hidden; width:100%;"></iframe>
    
</div>

</div>



<script type="text/javascript" src="@Url.Content("~/js/staticscreen.js")"></script>
</body>
</html>

  
